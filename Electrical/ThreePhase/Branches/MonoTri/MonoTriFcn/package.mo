within OpenIPSL.Electrical.ThreePhase.Branches.MonoTri;
package MonoTriFcn "Set of functions used in hybrid devices when finite Norton equivalent impedances are used"
  extends Modelica.Icons.Package;

annotation (
Documentation(info="<html>
<p>This package contains a set of functions used to calculate impedance matrices for hybrid components if positive-, negative- and zero-sequence Norton equivalent impedances are considered to have finite values.</p>
</html>"));
end MonoTriFcn;
